{
  "name": "idd",
  "version": "0.0.0",
  "description": "Injection of Dependencies through Destructuring",
  "main": "index.js",
  "scripts": {
    "clean": "del test/fixtures/**/build npm-debug.log* index.js",
    "compile": "babel index.babel.js -o index.js",
    "prepublish": "npm run compile && npm run test",
    "test": "ava && xo",
    "watch": "chokidar '**/.js' 'npm run compile && npm test'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doug-wade/idd.git"
  },
  "keywords": [
    "dependency",
    "injection",
    "di",
    "destructuring",
    "idd"
  ],
  "author": "Doug Wade <doug@dougwade.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doug-wade/idd/issues"
  },
  "homepage": "https://github.com/doug-wade/idd#readme",
  "dependencies": {
    "babylon": "^6.8.4"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chokidar-cli": "^1.2.0",
    "del-cli": "^0.2.0",
    "xo": "^0.16.0"
  },
  "ava": {
    "files": [
      "test/*.spec.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "xo": {
    "ignores": "index.js"
  }
}
