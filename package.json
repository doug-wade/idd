{
  "name": "idd",
  "version": "0.1.0",
  "description": "Injection of Dependencies through Destructuring",
  "main": "dist/index.js",
  "scripts": {
    "clean": "del test/fixtures/**/build npm-debug.log* dist/",
    "compile": "babel src -d dist",
    "prepublish": "npm run compile && npm run test",
    "test": "nyc ava && xo *.babel.js && nyc check-coverage --lines 80 --functions 90 --branches 79",
    "watch": "chokidar '*.babel.js' 'npm run compile && npm test'",
    "coverage": "nyc report --reporter lcov | codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doug-wade/idd.git"
  },
  "keywords": [
    "dependency",
    "injection",
    "di",
    "destructuring",
    "idd"
  ],
  "author": "Doug Wade <doug@dougwade.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doug-wade/idd/issues"
  },
  "homepage": "https://github.com/doug-wade/idd#readme",
  "dependencies": {
    "babel-traverse": "^6.13.0",
    "babel-types": "^6.13.0",
    "babylon": "^6.8.4",
    "chalk": "^1.1.3"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chokidar-cli": "^1.2.0",
    "codecov": "^1.0.1",
    "del-cli": "^0.2.0",
    "nyc": "^8.3.0",
    "xo": "^0.16.0"
  },
  "ava": {
    "files": [
      "test/*.spec.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "xo": {
    "ignores": "index.js"
  }
}
